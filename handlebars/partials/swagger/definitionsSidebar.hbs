{{!--
    Renders the definition-section of the HTML-page.
    @param {Definition[]} definitions a list of JSON-subschemas.
    @api public
--}}
                   
{{#if definitions}}
    <a name="/models"></a>
    <h4>Models</h4>
    <ul>
    {{#eachSorted definitions}}
        <li>{{>swagger/modelSidebar model=. title=@key anchor="/definitions" }}</li>
    {{/eachSorted}}
    </ul>
{{/if}}    
